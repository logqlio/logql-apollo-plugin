{
  "name": "@logql/apollo-plugin",
  "version": "0.0.2",
  "description": "Logql plugin for Apollo Server",
  "keywords": [
    "apollo",
    "graphql",
    "logql",
    "monitoring",
    "tracing"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/logqlio/logql-apollo-plugin"
  },
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "test": "jest -i --colors --verbose",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "fmt": "prettier --write ."
  },
  "dependencies": {
    "async-retry": "^1.2.1",
    "lru-cache": "^7.10.1",
    "node-fetch": "^2.6.7",
    "zod": "^3.20.2",
    "zod-validation-error": "^0.3.0"
  },
  "devDependencies": {
    "@apollo/gateway": "^2.2.2",
    "@apollo/server": "^4.1.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "graphql": "^16.6.0",
    "jest": "^29.3.1",
    "nock": "^13.2.9",
    "prettier": "^2.0.5",
    "supertest": "^6.3.3"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 100
      }
    }
  },
  "files": [
    "src/",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=14"
  }
}
